<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rtietjen2.trabalhoapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Telas.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Telas.MenuPrincipal" />
        <activity android:name=".Telas.TelaCliente" />
        <activity android:name=".Telas.TelaProfissional" />
        <activity android:name=".Telas.TelaProcedimento" />
        <activity android:name=".Telas.CadastroClienteActivity" />
        <activity android:name=".Telas.CadastroProfissionalActivity" />
        <activity android:name=".Telas.CadastroProcedimentoActivity" />
        <activity android:name=".Telas.TelaAgendamento" />
        <activity android:name=".Telas.CadastroAgendamentoActivity" />
        <activity android:name=".Telas.ClienteSelecionado"></activity>
        <activity android:name=".Telas.ProcedimentoSelecionado" />
        <activity android:name=".Telas.ProfissionalSelecionado" />
    </application>

</manifest>